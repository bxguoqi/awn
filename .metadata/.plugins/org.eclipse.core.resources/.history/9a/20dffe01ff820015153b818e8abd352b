 */
package base;

import java.util.List;
import java.util.Set;

/**
 * @author Administrator
 *
 */
public class Tnode {
	public Tnode(){
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	//表达式的中文标示
	String content;
	//后继节点
	List<Link> succNodes;
	//前驱节点
	List<Link> preNodes;
	/*def: 这条语句定义的变量
	 * ref: 这条语句引用的变量
	 * */
	Set<Var> def,ref;
}
